{
  "configuration": {
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%highlight{%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5level] [%t] %c{10}  - %msg%n}{FATAL=red blink, ERROR=red, WARN=yellow bold, INFO=green bold, DEBUG=black, TRACE=blue}"
        }
      },
      "RollingFile": {
        "name": "File",
        "fileName": "/home/doan/Documents/intelin/EveryThing/allthing.log",
        "filePattern": "/home/doan/Documents/intelin/EveryThing/allthing-backup-%d{MM-dd-yy-HH-mm-ss}-%i.log.gz",
        "PatternLayout": {
          "pattern": "%d{yyyy-MM-dd HH:mm:ss} %-5p %c{1}:%L - %m%n"
        },
        "Policies": {
          "SizeBasedTriggeringPolicy": {
            "size": "10 MB"
          }
        },
        "DefaultRolloverStrategy": {
          "max": "10"
        }
      },
      "Socket":{
        "name":"socket",
        "host":"192.168.18.49",
        "port":9600
      }
    },
    "loggers": {
      "root": {
        "level": "trace",
        "AppenderRef": [
          {
            "ref": "STDOUT"
          },
          {
            "ref": "File"
          },
          {
            "ref":"socket"
          }
        ]
      }
    }
  }
}